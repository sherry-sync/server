{
  "name": "sherry-server",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "git@personal.github.com:sherry-sync/server.git",
  "author": "andrii.yevtushenko <andrii.yevtushenko@strg.at>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "prepare": "prisma generate --schema ./src/prisma/schema.prisma",
    "prisma:format": "prisma format --schema ./src/prisma/schema.prisma",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "migrate:dev": "prisma migrate dev --schema ./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/helmet": "^11.1.1",
    "@nestjs/common": "^10.3.3",
    "@nestjs/core": "^10.3.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.3.3",
    "@prisma/client": "^5.10.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.1",
    "fastify-multer": "^2.0.3",
    "fastify-multipart": "^5.4.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@types/node": "^20.11.24",
    "@types/passport-jwt": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
